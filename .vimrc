set nocompatible
filetype plugin on
filetype indent on
set autoread
set clipboard=unnamed,unnamedplus
set title
set scrolloff=5
set number
set relativenumber
set ruler
set hidden
set backspace=eol,start,indent
set ignorecase
set smartcase
set hlsearch
set incsearch
set showmatch
syntax enable
set encoding=utf-8
set background=dark
